	x:Class="E.Dairy.WPF.UserControl1"
	x:Name="UserControl"
	d:DesignWidth="492.84" d:DesignHeight="161">

	<Grid x:Name="LayoutRoot">
		<Custom:Calendar FontSize="18.667" FontFamily="Verdana" HorizontalAlignment="Left" VerticalAlignment="Top" FontStretch="SemiExpanded" FontWeight="Bold">
